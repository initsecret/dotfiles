{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "editor.fontSize": 14,
    "window.zoomLevel": 2,
    "editor.rulers": [
        80,
        120
    ],
    "cSpell.userWords": [
        "microcontrollers"
    ],
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "boundary",
    "editor.largeFileOptimizations": false,
    "window.title": "${dirty}${activeEditorMedium}",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                ":w"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "commands": [
                ":wq"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<leader>",
                "v"
            ],
            "commands": [
                ":tabnew ~/Library/Application Support/Code/User/settings.json"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "files.eol": "\n",
    "editor.formatOnSaveTimeout": 5000,
    "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "[cpp]": {
        "editor.formatOnSave": true,
    },
    "[c]": {
        "editor.formatOnSave": true,
    },
    "[html]": {
        "editor.formatOnSave": false,
    },
    "[js]": {
        "editor.formatOnSave": false,
    },
    "readOnlyModeSupport.ignore": [
        "/Users/sanketh/Library/Application Support/Code/User/settings.json"
    ],
    "clang-format.fallbackStyle": "Google",
    "editor.formatOnSave": true,
    "python.formatting.provider": "black",
    "[javascript]": {
        "editor.formatOnSave": false
    },
}
